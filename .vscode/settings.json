{
  "deno.enable": true,
  "deno.lint": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.removeUnusedImports": "explicit",
    "source.sortImports": "explicit"
  },
  "editor.defaultFormatter": "denoland.vscode-deno",
  "markdownlint.config": {
    "single-h1": false
  }
}
